$("#new_container_plus").hide().after("<%=j render("form") %>");
$("#new_container_label").html("Editing Container: <%=j @container.name %>");
$(".back-link").on('click', function() {
    $(".back-link").remove();
    $(".edit_container").remove();
    $("#new_container_plus").fadeIn(.500);
    $("#new_container_label").html("New Container");
});
