<% @staging.each do |staging| %>
  <span><%= staging.name %></span>
  <% if on_my_page? %><%= link_to "", new_container_link_path(@staging), class:"glyphicon glyphicon-plus add-staging" %><% end%>
  <hr id="welcome-page-hr">
  <table class="staging-left">
  <% staging.links.each do |link| %>
  <tbody>
    <tr>
      <td colspan="2"><%= link_to link.name, url_with_protocol(link.url), :target => '_blank' %>
      <% if on_my_page? %>
        <%= link_to '', [staging, link], method: :delete, data: { confirm: 'Are you sure?' },
          class:"glyphicon glyphicon-trash container-icon" %>
        <%= link_to '', edit_container_link_path(staging, link), class: "glyphicon glyphicon-edit container-icon" %>
      </td><% end %>
    </tr>
    <tr>
      <td><%= link.description %></td>
    </tr>
  <tbody>
  <% end %>
  </table>

<% end %>

