<% if on_my_page? %>
  <div class="users-show">
    <%= render 'users/layouts/usersnav' %>
    <div class="col-sm-3 col-md-3 col-lg-3" id="custom-well-left">
      <%= render 'users/layouts/leftcolumn' %>
    </div>
    <div class="col-lg-9 col-md-9" id="all_containers">
      <%= render 'users/layouts/rightcolumn' %>
    </div>
<% else %>
  <%= render 'users/layouts/usersnav' %>
    <div class="container">
      <%= render 'users/layouts/rightcolumn' %>
    </div>
  </div>
<% end %>




