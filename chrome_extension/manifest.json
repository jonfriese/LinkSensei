{
  "manifest_version": 2,

  "name": "LinkSensei",
  "description": "This extension will provide content to your LinkSensei account.",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["getTabs.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "Import your tabs",
    "default_icon": "icon.png"
  }
}
